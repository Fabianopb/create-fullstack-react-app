{
  "version": "1.3.2",
  "name": "create-fullstack-react-app",
  "keywords": [
    "react",
    "fullstack",
    "express"
  ],
  "description": "Fullstack React application generator.",
  "files": [
    "build",
    "templates"
  ],
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.20.*"
  },
  "bin": "./build/index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "deepmerge": "^4.2.2",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx,css}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/*.test.js"
    ]
  },
  "scripts": {
    "create-sample": "yarn clean && SKIP_PREFLIGHT_CHECK=true babel-node src/index.js sample-app",
    "build": "babel ./src --out-dir build --ignore \"src/*.test.js\" --presets minify",
    "lint": "eslint ./",
    "test": "jest",
    "e2e": "babel-node tools/e2e.js",
    "clean": "rimraf sample-app",
    "prepublish": "yarn build"
  }
}
