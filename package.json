{
  "version": "1.2.0",
  "name": "create-fullstack-react-app",
  "keywords": [
    "react",
    "fullstack",
    "express"
  ],
  "description": "Fullstack React application generator.",
  "files": [
    "build",
    "templates"
  ],
  "bin": "./build/index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "deepmerge": "^4.2.2",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx,css}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/*.test.js"
    ]
  },
  "scripts": {
    "create-sample": "yarn clean && babel-node src/index.js sample-app",
    "build": "babel ./src --out-dir build --ignore \"src/*.test.js\" --presets minify",
    "lint": "eslint ./src ./templates",
    "test": "jest",
    "e2e": "babel-node tools/e2e.js",
    "clean": "rimraf sample-app",
    "prepublish": "yarn build"
  }
}
