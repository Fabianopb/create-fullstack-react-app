{
  "name": "postgresql-server",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.6.*",
    "yarn": ">=1.10.*"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "axios-mock-adapter": "^1.17.0",
    "babel-preset-minify": "^0.5.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start-server": "cd build-server && node server.js",
    "dev-server": "nodemon --exec babel-node server/server.js",
    "test-server": "jest --config=server/jest.config.js /server --coverage",
    "build-server": "babel ./server --out-dir build-server --ignore \"server/coverage\",\"server/**/*.test.js\",\"server/jest.config.js\" --presets minify"
  }
}
