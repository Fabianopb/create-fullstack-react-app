{
  "name": "mongodb-server",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.6.*",
    "yarn": ">=1.10.*"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "axios-mock-adapter": "^1.17.0",
    "chalk": "^3.0.0",
    "jest": "^24.9.0",
    "mongodb-memory-server": "^6.2.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start-server": "cd build-server && node server.js",
    "dev-server": "nodemon --exec babel-node server/server.js",
    "test-server": "jest --config=server/jest.config.js /server --coverage",
    "build-server": "babel ./server --out-dir build-server --ignore \"server/coverage\",\"server/**/*.test.js\",\"server/jest.config.js\""
  }
}
